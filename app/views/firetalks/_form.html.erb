<%= form_for [@user, @firetalk] do |f| %>
  <%= f.hidden_field :user_id, :value => @user.id %>
  <%= f.text_field :topic %>
  <%= f.text_field :name %>
  <%= f.text_field :description %>
  <%= f.text_field :rounds, :value => 5 %>
  <%= f.text_field :seconds %>
  <%= f.fields_for :firetalk_debaters do |ff| %>
    <%= ff.text_field :email, :placeholder => "invite debater" %>
  <% end %>
  <%= f.submit %>
<% end %>
